{
  "name": "my-module",
  "version": "1.0.0",
  "description": "My new Nuxt module",
  "repository": "your-org/my-module",
  "license": "MIT",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/types.d.mts",
      "import": "./dist/module.mjs"
    }
  },
  "main": "./dist/module.mjs",
  "typesVersions": {
    "*": {
      ".": [
        "./dist/types.d.mts"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "prepack": "nuxt-module-build build",
    "dev": "nuxi dev playground",
    "dev:build": "nuxi build playground",
    "dev:prepare": "nuxt-module-build build --stub && nuxt-module-build prepare && nuxi prepare playground",
    "release": "npm run lint && npm run test && npm run prepack && changelogen --release && npm publish && git push --follow-tags",
    "test": "vitest run",
    "test:watch": "vitest watch",
    "test:types": "vue-tsc --noEmit && cd playground && vue-tsc --noEmit"
  },
  "dependencies": {
    "@alireza-ab/vue3-persian-datepicker": "^1.0.5",
    "@headlessui/vue": "^1.7.16",
    "@heroicons/vue": "^2.0.18",
    "@nuxt/kit": "^3.9.3",
    "@pinia/nuxt": "^0.11.1",
    "@tailvue/nuxt": "^1.0.0",
    "@tiptap/extension-image": "^2.10.2",
    "@tiptap/extension-link": "^2.10.0",
    "@tiptap/pm": "^2.0.3",
    "@tiptap/starter-kit": "^2.0.3",
    "@tiptap/vue-3": "^2.0.3",
    "@vueuse/core": "^13.2.0",
    "axios": "^1.9.0",
    "date-fns": "^4.1.0",
    "defu": "^6.1.4",
    "formidable": "^3.5.4",
    "html2canvas": "^1.4.1",
    "imask": "^7.6.1",
    "lodash": "^4.17.21",
    "nanoid": "^5.1.5",
    "nuxt-tailvue": "^1.0.95",
    "nuxt-tv-toast": "^1.0.5",
    "pinia": "^3.0.2",
    "sass": "^1.81.0",
    "sharp": "^0.34.2",
    "tailwindcss": "^3.4.15",
    "ua-parser-js": "^2.0.3",
    "vue-imask": "^7.6.1",
    "vue-sweetalert2": "^5.0.5",
    "vue3-slide-up-down": "^2.1.0",
    "vuedraggable": "^4.1.0",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "@nuxt/devtools": "^2.4.1",
    "@nuxt/icon": "^1.15.0",
    "@nuxt/module-builder": "^1.0.1",
    "@nuxt/schema": "^3.9.3",
    "@nuxt/test-utils": "^3.11.0",
    "@nuxtjs/tailwindcss": "^6.14.0",
    "@prisma/client": "^6.12.0",
    "@types/node": "^20.11.19",
    "changelogen": "^0.5.5",
    "nuxt": "^3.9.3",
    "prisma": "^6.12.0",
    "typescript": "~5.8.3",
    "vitest": "^3.1.4",
    "vue-tsc": "^2.2.10"
  }
}
